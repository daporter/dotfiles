defaults
port 587
protocol smtp
auth on
tls on
tls_trust_file /usr/local/etc/openssl/cert.pem

account personal
host smtp.gmail.com
user david.a.porter@gmail.com
from david.a.porter@gmail.com
passwordeval /usr/bin/security find-internet-password -w -a david.a.porter@gmail.com -s smtp.gmail.com ~/Library/Keychains/login.keychain
